    "monit": "npx pm2 monit express-pm2-long-polling",
    "graceful": "npx pm2 startOrGracefulReload pm2.config.js",
    "stop": "npx pm2 stop pm2.config.js",
    "build": "babel ./src --out-dir ./build",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.*\"",
    "lint:prettier": "prettier --check \"**/*.*\"",
    "lint:fix": "eslint . --fix",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint --fix \"**/*.*\"",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "debug": "node --inspect-brk src/server.js",
    "ping": "./script/test-ping.sh"
  },
  "dependencies": {
    "bunyan": "~1.8.15",
    "cors": "~2.8.5",
